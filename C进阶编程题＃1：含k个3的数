#include <iostream>
#include <stdlib.h>
#include <stdio.h>
#include <string.h>
using namespace std;



int main(){
    int m, k;
    cin>>m;
    cin>>k;
    int n=m;
    int t=0;
    for(int i=0; i<5; i++){
            if(n%10==3){
                t+=1;
                n=n/10;
            }
            else{
                n=n/10;
            }
        }

    if(m%19==0&&k==t){
        cout<<"YES"<<endl;
    }
    else{
        cout<<"NO"<<endl;
    }
    return 0;


}
