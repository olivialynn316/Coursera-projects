#include <iostream>
#include <stdlib.h>
#include <stdio.h>
#include <string.h>
using namespace std;


int main(){
    int a, b, c;
    scanf("%d,%d,%d",&a, &b, &c);
    if(a+b==c){
        cout<<'+'<<endl;
    }
    else if(a-b==c){
        cout<<'-'<<endl;
    }
    else if(a*b==c){
        cout<<'*'<<endl;
    }
    else if(a/b==c&&a%b==0&&b!=0){
        cout<<'/'<<endl;
    }
    else if(b!=0&&a%b==c){
        cout<<'%'<<endl;
    }
    else{
        cout<<"error"<<endl;
    }



    return 0;

}
